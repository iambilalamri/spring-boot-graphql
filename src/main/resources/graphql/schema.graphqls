schema {
	query: Query
	mutation:Mutation
}

type Actor {
	actorId:ID!
	firstname:String
	lastname:String
	dob:String
	address:String
	filmId:Int
	film:Film
}

type Film {
	filmId:ID!
	name:String
	launchDate:String
}

type Query {
	getAllActors:[Actor]
	getActorById(id:Int):Actor
	getAllFilms:[Film]
}

type Mutation {
	updateAddress(id:Int, address:String):Actor
}